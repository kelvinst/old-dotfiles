[user]
	email = kelvin.stinghen@gmail.com
	name = kelvinst
[alias]
	co = checkout
	lg = log --all --graph --decorate --oneline --abbrev-commit
	cm = commit
	ac = !git add -A && git commit
	st = status -sb
	tags = tag -l
	branches = branch -a
	remotes = remote -v
[push]
	default = simple
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[core]
	editor = /usr/bin/vim
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[difftool]
	prompt = false
